{
  "name": "buildman",
  "version": "0.0.1",
  "bin": {
    "buildman": "bin/buildman"
  },
  "scripts": {
    "build": "npm run lint && npm test",
    "test": "npm run test:unit && npm run test:integration",
    "test:integration": "tape 'test/integration/**/*.test.js'",
    "test:unit": "tape 'test/unit/**/*.test.js'",
    "lint": "eslint ."
  },
  "devDependencies": {
    "eslint": "^4.3.0",
    "eslint-config-xo": "^0.18.2",
    "sinon": "^2.4.1",
    "tape": "^4.7.0"
  },
  "dependencies": {
    "deep-equal": "^1.0.1"
  }
}
