{
  "name": "whatsdone-app",
  "private": true,
  "engines": {
    "node": ">= 16"
  },
  "config": {
    "BUILD_DIR_NAME": "dist"
  },
  "scripts": {
    "build": "yarn && yarn run compile && ./build.sh",
    "compile": "tsc -p ./",
    "test": "mocha",
    "lint": "tslint -p tslint.json",
    "coverage": "nyc yarn test",
    "predeploy": "yarn run build",
    "deploy": "cd ops && yarn && kumo deploy-module --verbose",
    "destroy": "cd ops && kumo destroy-module --verbose"
  },
  "dependencies": {
    "aws-sdk": "~2.820.0",
    "aws-xray-sdk": "^3.2.0",
    "lodash.get": "~4.4.2",
    "lodash.keyby": "~4.6.0",
    "lodash.map": "~4.6.0",
    "lodash.omit": "~4.5.0",
    "lodash.pick": "~4.4.0",
    "route-parser": "0.0.5",
    "uuid": "~8.3.2"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.keyby": "^4.6.6",
    "@types/lodash.map": "^4.6.13",
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/mocha": "^8.2.0",
    "@types/node": "^12.19.11",
    "@types/route-parser": "^0.1.3",
    "@types/uuid": "^8.3.0",
    "mocha": "^8.2.1",
    "nyc": "^15.1.0",
    "testdouble": "^3.16.1",
    "ts-node": "^9.1.1",
    "tslint": "^6.1.3",
    "typescript": "^4.1.3"
  }
}
