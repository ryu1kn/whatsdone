{
  "name": "whatsdone-app",
  "private": true,
  "engines": {
    "node": ">= 12"
  },
  "config": {
    "BUILD_DIR_NAME": "dist"
  },
  "scripts": {
    "build": "yarn && yarn run compile && ./build.sh",
    "compile": "tsc -p ./",
    "test": "mocha --opts mocha.opts",
    "lint": "tslint -p tslint.json",
    "coverage": "nyc yarn test",
    "predeploy": "yarn run build",
    "deploy": "cd ops && yarn && kumo deploy-module --verbose",
    "destroy": "cd ops && kumo destroy-module --verbose"
  },
  "dependencies": {
    "aws-sdk": "~2.656.0",
    "aws-xray-sdk": "^3.2.0",
    "lodash.get": "~4.4.2",
    "lodash.keyby": "~4.6.0",
    "lodash.map": "~4.6.0",
    "lodash.omit": "~4.5.0",
    "lodash.pick": "~4.4.0",
    "route-parser": "0.0.5",
    "uuid": "~7.0.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.11",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.keyby": "^4.6.6",
    "@types/lodash.map": "^4.6.13",
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/mocha": "^7.0.2",
    "@types/node": "^12.0.10",
    "@types/route-parser": "^0.1.3",
    "@types/sinon": "^9.0.0",
    "@types/uuid": "^7.0.2",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "nyc": "^15.0.1",
    "sinon": "^9.0.2",
    "testdouble": "^3.13.1",
    "ts-node": "^8.8.2",
    "tslint": "^6.1.1",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "**/nyc/**/minimist": "^1.2.5"
  }
}
