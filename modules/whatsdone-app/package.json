{
  "name": "whatsdone-app",
  "private": true,
  "engines": {
    "node": ">= 8"
  },
  "config": {
    "BUILD_DIR_NAME": "dist"
  },
  "scripts": {
    "build": "yarn && yarn run compile && ./build.sh",
    "compile": "tsc -p ./",
    "test": "mocha --opts mocha.opts",
    "lint": "tslint -p tslint.json",
    "coverage": "nyc yarn test",
    "predeploy": "yarn run build",
    "deploy": "cd ops && yarn && kumo deploy-module --verbose",
    "destroy": "cd ops && kumo destroy-module --verbose"
  },
  "dependencies": {
    "aws-sdk": "~2.596.0",
    "aws-xray-sdk": "~1.2.0",
    "lodash.get": "~4.4.2",
    "lodash.keyby": "~4.6.0",
    "lodash.map": "~4.6.0",
    "lodash.omit": "~4.5.0",
    "lodash.pick": "~4.4.0",
    "route-parser": "0.0.5",
    "uuid": "~3.3.3"
  },
  "devDependencies": {
    "@types/chai": "^4.2.7",
    "@types/lodash.get": "^4.4.6",
    "@types/lodash.keyby": "^4.6.6",
    "@types/lodash.map": "^4.6.13",
    "@types/lodash.omit": "^4.5.6",
    "@types/lodash.pick": "^4.4.6",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.0.10",
    "@types/route-parser": "^0.1.3",
    "@types/sinon": "^7.5.1",
    "@types/uuid": "^3.4.6",
    "chai": "^4.2.0",
    "mocha": "^6.2.2",
    "nyc": "^15.0.0",
    "sinon": "^8.0.1",
    "testdouble": "^3.12.4",
    "ts-node": "^8.5.4",
    "tslint": "^5.20.1",
    "typescript": "^3.7.4"
  }
}
