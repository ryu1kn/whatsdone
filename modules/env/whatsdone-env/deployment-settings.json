{
  "moduleName": "whatsdone-env",
  "config": {
    "script": "node -p \"JSON.stringify(require('./module-config/$KUMO_ENV.js'))\""
  },
  "tasks": [
    {
      "id": "prepare-root-bucket",
      "type": "cf-stack",
      "stackName": "module",
      "stackTemplate": {
        "script": "cp ./template.json $KUMO_TEMPLATE_OUTPUT_FILE"
      },
      "stackParams": {
        "BucketName": {"$ref": "#/_deploymentConfig/bucketName"}
      }
    }
  ]
}
