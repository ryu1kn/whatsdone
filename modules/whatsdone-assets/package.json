{
  "name": "whatsdone-assets",
  "dependencies": {
    "flux": "~3.1.2",
    "keymirror": "~0.1.1",
    "lodash": "~4.17.3",
    "react": "~15.4.1",
    "react-dom": "~15.4.1",
    "react-router-dom": "^4.1.1",
    "whatwg-fetch": "^2.0.3"
  },
  "engines": {
    "node": "6.10"
  },
  "config": {
    "buildDir": "./dist"
  },
  "scripts": {
    "prestart:dev": "npm run webpack && cp app-config/default.js $npm_package_config_buildDir/app.config.js",
    "start:dev": "webpack-dev-server --content-base $npm_package_config_buildDir",
    "start:api": "node api-server.js",
    "deploy": "kumo deploy-module --verbose",
    "prebuild": "rm -rf $npm_package_config_buildDir",
    "build": "npm run webpack",
    "webpack": "webpack --optimize-minimize",
    "lint": "eslint src"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "cors": "^2.8.3",
    "css-loader": "^0.28.4",
    "eslint": "^3.12.2",
    "eslint-config-xo": "^0.17.0",
    "eslint-plugin-react": "^6.8.0",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "favicons-webpack-plugin": "0.0.7",
    "html-webpack-plugin": "^2.29.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "style-loader": "^0.18.2",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
