{
  "name": "whatsdone-assets-app",
  "private": true,
  "types": "src/main.d.ts",
  "engines": {
    "node": ">= 14"
  },
  "config": {
    "buildDir": "./dist"
  },
  "scripts": {
    "prestart:dev": "yarn run build:dev && cp ../app-config/dev.json $npm_package_config_buildDir/appConfig.json",
    "start:dev": "webpack server --mode development --content-base $npm_package_config_buildDir",
    "start:api": "node api-server.js",
    "prebuild:dev": "yarn run clean",
    "build:dev": "NODE_ENV=dev webpack",
    "prebuild:production": "yarn run clean",
    "build:production": "NODE_ENV=production webpack",
    "postbuild:dev": "yarn run remove-dup-static",
    "postbuild:production": "yarn run remove-dup-static",
    "remove-dup-static": "mv $npm_package_config_buildDir/static/static/* $npm_package_config_buildDir/static && rmdir $npm_package_config_buildDir/static/static # HACK",
    "test": "jest",
    "clean": "rm -rf $npm_package_config_buildDir"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^1.31.0",
    "aws-sdk": "^2.931.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^3.3.7",
    "jquery": "^3.6.0",
    "jwt-decode": "^3.1.2",
    "lodash": "~4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.2.0",
    "redux": "^4.1.0",
    "showdown": "^1.9.1",
    "util": "^0.12.4",
    "whatwg-fetch": "~3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.14.5",
    "@babel/register": "^7.14.5",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.170",
    "@types/react": "^17.0.11",
    "@types/react-dom": "^17.0.8",
    "@types/react-router-dom": "^5.1.7",
    "babel-jest": "^27.0.2",
    "babel-loader": "^8.2.2",
    "cors": "^2.8.4",
    "css-loader": "^5.2.6",
    "express": "^4.15.4",
    "favicons": "^6.2.2",
    "favicons-webpack-plugin": "^5.0.2",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.0.4",
    "json-loader": "^0.5.7",
    "less": "^4.1.1",
    "less-loader": "^10.0.0",
    "mini-css-extract-plugin": "^1.6.0",
    "style-loader": "^2.0.0",
    "testdouble": "^3.16.1",
    "typescript": "^4.3.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.39.1",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  },
  "jest": {
    "cacheDirectory": ".cache/jest",
    "testPathIgnorePatterns": [
      "<rootDir>/dist/",
      "<rootDir>/.cache/"
    ]
  }
}
