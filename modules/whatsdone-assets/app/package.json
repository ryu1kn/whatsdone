{
  "name": "whatsdone-assets-app",
  "dependencies": {
    "lodash": "~4.17.3",
    "react": "~15.4.1",
    "react-dom": "~15.4.1",
    "react-redux": "~5.0.5",
    "react-router-dom": "~4.1.1",
    "redux": "~3.7.2",
    "showdown": "^1.7.2",
    "whatwg-fetch": "~2.0.3"
  },
  "engines": {
    "node": "6.10"
  },
  "config": {
    "buildDir": "./dist"
  },
  "scripts": {
    "prestart:dev": "npm run build:dev && cp ../app-config/dev.json $npm_package_config_buildDir/appConfig.json",
    "start:dev": "webpack-dev-server --content-base $npm_package_config_buildDir",
    "start:api": "node api-server.js",
    "prebuild:dev": "npm run clean",
    "build:dev": "NODE_ENV=dev webpack",
    "prebuild:production": "npm run clean",
    "build:production": "NODE_ENV=production webpack",
    "postbuild:production": "mv $npm_package_config_buildDir/static/static/* $npm_package_config_buildDir/static && rmdir $npm_package_config_buildDir/static/static # HACK",
    "test": "tape --require babel-register test/**/*.test.js",
    "clean": "rm -rf $npm_package_config_buildDir"
  },
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "cors": "^2.8.3",
    "css-loader": "^0.28.4",
    "express": "^4.15.3",
    "extract-text-webpack-plugin": "^2.1.2",
    "favicons-webpack-plugin": "0.0.7",
    "html-webpack-plugin": "^2.29.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "sinon": "^2.3.8",
    "style-loader": "^0.18.2",
    "tape": "^4.7.0",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0"
  }
}
