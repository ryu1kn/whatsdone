{
  "name": "whatsdone-assets-app",
  "private": true,
  "engines": {
    "node": ">= 12"
  },
  "config": {
    "buildDir": "./dist"
  },
  "scripts": {
    "prestart:dev": "yarn run build:dev && cp ../app-config/dev.json $npm_package_config_buildDir/appConfig.json",
    "start:dev": "webpack-dev-server --content-base $npm_package_config_buildDir",
    "start:api": "node api-server.js",
    "prebuild:dev": "yarn run clean",
    "build:dev": "NODE_ENV=dev webpack",
    "prebuild:production": "yarn run clean",
    "build:production": "NODE_ENV=production webpack",
    "postbuild:dev": "yarn run remove-dup-static",
    "postbuild:production": "yarn run remove-dup-static",
    "remove-dup-static": "mv $npm_package_config_buildDir/static/static/* $npm_package_config_buildDir/static && rmdir $npm_package_config_buildDir/static/static # HACK",
    "test": "tape --require babel-register test/**/*.test.js",
    "clean": "rm -rf $npm_package_config_buildDir"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "^1.31.0",
    "aws-sdk": "^2.184.0",
    "babel-polyfill": "^6.26.0",
    "bootstrap": "^3.3.7",
    "jquery": "^3.5.0",
    "jwt-decode": "^2.2.0",
    "lodash": "~4.17.21",
    "react": "~15.4.1",
    "react-dom": "~15.4.1",
    "react-redux": "~5.0.5",
    "react-router-dom": "~4.1.1",
    "redux": "~3.7.2",
    "showdown": "^1.9.1",
    "whatwg-fetch": "~2.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.14.6",
    "@babel/preset-env": "^7.14.5",
    "@babel/preset-react": "^7.13.13",
    "@babel/register": "^7.14.5",
    "babel-loader": "^8.2.2",
    "cors": "^2.8.4",
    "css-loader": "^0.28.5",
    "express": "^4.15.4",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "0.0.7",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.30.1",
    "json-loader": "^0.5.7",
    "less": "^2.7.2",
    "less-loader": "^4.0.5",
    "style-loader": "^0.18.2",
    "tape": "^5.1.1",
    "testdouble": "^3.16.1",
    "url-loader": "^0.5.9",
    "webpack": "^3.5.5",
    "webpack-dev-server": "^3.1.11"
  }
}
